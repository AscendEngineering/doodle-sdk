
config wifi-device 'radio0'
	option type 'mac80211'
	option hwmode '11g'
	option path 'platform/qca953x_wmac'
	option htmode 'HT20'
	option fes_disabled '0'
	option country 'GV'
	option distance '4000'
	option rxantenna '1 2'
	option txantenna '1 2'
	option channel '12'
	option disabled '0'
	option legacy_rates '0'
	option chanbw '26'
	option dynamic_txpower '1'
	option dynamic_txpower_aggr '1'

config wifi-device 'radio1'
	option type 'mac80211'
	option path 'pci0000:00/0000:00:00.0'
	option htmode 'HT20'
	option country 'GV'
	option force_ofdm '0'
	option chanbw '0'
	option dynamic_txpower '0'
	option dynamic_txpower_aggr '0'
	option hwmode '11a'
	option rxantenna '1'
	option txantenna '1'
	option disabled '0'
	option legacy_rates '0'
	option channel 'auto'

config wifi-iface 'wifi0'
	option device 'radio0'
	option mode 'mesh'
	option network 'mesh_dev'
	option mesh_id 'simpleconfig'
	option mesh_fwding '0'
	option mesh_nolearn '1'
	option mesh_ttl '31'
	option mcast_rate '12000'
	option encryption 'psk2+ccmp'
	option key 'DoodleSmartRadio'
	option mesh_cwmin_vi '5'
	option mesh_cwmax_vi '6'
	option mesh_cwmin_vo '1'
	option mesh_cwmax_vo '2'
	option mesh_aifs_vi '8'
	option mesh_aifs_vo '2'
	option mesh_aifs_bk '8'
	option mesh_aifs_be '8'
	option mesh_cwmin_be '5'
	option mesh_cwmax_be '6'

config wifi-iface 'wifi1'
	option device 'radio1'
	option ssid 'DoodleLabsWiFi'
	option key 'DoodleSmartRadio'
	option encryption 'psk2+ccmp'
	option ieee80211r '1'
	option network 'wan'
	option multicast_to_unicast '1'
	option mode 'ap'

